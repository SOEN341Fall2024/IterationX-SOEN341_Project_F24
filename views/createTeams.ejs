<%- include('partials/header', { title: 'Instructor Dashboard' }) %>
<nav>
    | <a href = "/instructor-dashboard"> Dashboard</a>|
    | <a href = "/viewTeams"> View Created Teams</a>|
    | <a href = "/logout"> Logout</a>|
</nav>
<form action="/createTeams" method="POST">
    <label for="teamname">Team Name</label>
    <input type="text" name="teamname" required />
    <table>
        <tr>
            <th>Student name</th><th>Student ID</th><th>Enroll</th></tr>
        </tr>
    <% for(var student in StudentArr) { %>
        <tr>
            <td><%= student %></td><td><%= studentID %></td><td><input type = "checkbox" name = "studentIDs" value = <%= studentID %></td>
        </tr>
    <% } %>
    </table>
    <button type="submit">Confirm choice</button>
</form>
<%- include('partials/footer') %>